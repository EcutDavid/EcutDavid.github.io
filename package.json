  "version": "0.0.1",
  "main": "",
  "scripts": {
    "clean": "rimraf assets && rimraf dist",
    "copy": "copyfiles -f ./src/favicon.ico ./dist",
    "publish": " npm run clean && npm run dist && cp -R ./dist/assets/ ./assets && cp ./dist/favicon.ico ./favicon.ico && cp ./dist/index.html ./index.html",
    "watch:dist": "webpack --env=dist --watch",
    "dist": "rimraf assets && npm run copy & webpack --env=dist",
    "profile:build": "rimraf assets && npm run copy & webpack --env=dist --display-modules",
    "lint": "eslint ./src",
    "posttest": "npm run lint",
    "serve": "node devServer.js --env=dev",
    "serve:dist": "node devServer.js --env=dist",
    "start": "webpack --watch",
    "test": "karma start",
    "test:watch": "karma start --autoWatch=true --singleRun=false"
  },
